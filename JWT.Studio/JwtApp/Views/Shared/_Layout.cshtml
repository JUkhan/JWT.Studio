<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")



</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Application name", "Index", "Home", null, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="#/Student">Student</a></li>
                    <li><a href="#/Course">Course</a></li>
                    <li><a href="#/Enrollment">Enrollment</a></li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        <div ui-view>@RenderBody()</div>
      
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
    <div class="overlay">
        <div class="jwt-spinner"><i class="fa fa-refresh fa-spin"></i></i></div>
    </div>

    <script src="JwtEx/Resources/?file=traceur.js"></script>
    <script src="JwtEx/Resources/?file=jquery.js"></script>
    <script src="JwtEx/Resources/?file=angular.js"></script>
    <script src="JwtEx/Resources/?file=angular-resource.js"></script>
    <script src="JwtEx/Resources/?file=angularui.js"></script>
    <script src="JwtEx/Resources/?file=angular-ui-router.js"></script>
    <script src="JwtEx/Resources/?file=angular-sanitize.js"></script>   
   
    <script type="module" src="~/Scripts/appStarter.js">
    </script>
    @Scripts.Render("~/bundles/uigrid")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqPlot")
    <script src="JwtEx/Resources/?file=jwt.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
