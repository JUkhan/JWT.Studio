<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")



</head>
<body ng-app="app" ng-controller="mainCtrl">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Application name", "Index", "Home", null, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @*<li><a href="{{url('nav1')}}">Nav1</a></li>
                    <li><a href="{{url('nav2')}}">Nav2</a></li>
                    <li><a href="{{url('nav3')}}">Nav3</a></li>*@
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        <div >@RenderBody()</div>
        <b>{{1+4}}</b>
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
    <div class="overlay">       
        <div class="jwt-spinner"><i class="fa fa-refresh fa-spin"></i></i></div>
    </div>
    
    <script src="JwtEx/Resources/?file=jquery.js"></script>
    
  
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/angular-ui-router.js"></script>
    <script src="JwtEx/Resources/?file=angular-sanitize.js"></script>
    <script src="JwtEx/Resources/?file=toastr.toastr.js"></script>

    @*<script src="JwtEx/Resources/?file=angular-resource.js"></script>
    <script src="JwtEx/Resources/?file=angularui.js"></script>
    <script src="JwtEx/Resources/?file=angular-ui-router.js"></script>
    <script src="JwtEx/Resources/?file=angular-sanitize.js"></script>*@  
    @*<script src="JwtEx/Resources/?file=jwt.js"></script>*@
    @*@Scripts.Render("~/bundles/controllers")*@
    <script src="~/Scripts/app.js"></script>
    <script src="JwtEx/Resources/?file=jwt.js"></script>
    @Scripts.Render("~/bundles/controllers")

    <script src="~/Scripts/router.js"></script>
   
    @RenderSection("scripts", required: false)
</body>
</html>
